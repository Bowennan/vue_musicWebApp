html,body,div,header,footer,nav,article,section,aside,ul,li,a,span,h1,h2,h3,h4,p,img
	margin:0
    padding:0
    font-size:100%
    vertical-align:baseline


html,body 
	width:100%
	height:100%
	font: 12px/1 Tahoma,Helvetica,Arial,'宋体',sans-serif
	overflow: hidden
	position: relative
	z-index: -100


a 
	text-decoration: none
	color:#fff

a:active 
	color: #ff890f


ul li 
	list-style: none


.clearfix 
	zoom:1

.clearfix:after 
	display: block
	content: ""
	clear: both
